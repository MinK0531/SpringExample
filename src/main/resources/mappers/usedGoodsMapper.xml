<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mink.springexamlpe.database.repository.UsedGoodsRepository">

    <select id="selectUsedGoodsList"
            resultType="com.mink.springexamlpe.database.domain.UsedGoods">
        SELECT
            `id`
             , `sellerId`
             , `title`
             , `price`
             , `description`
             , `image`
             , `createdAt`
             , `updatedAt`
        FROM
            `used_goods`
    </select>

</mapper>